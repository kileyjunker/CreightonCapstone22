    "import numpy as np\n",
    "import random\n",
    "from ripser import ripser\n",
    "from persim import plot_diagrams\n",
    "\n",
    "#Distance between points using max\n",
    "#x: a point from P1 (x,y)\n",
    "#y: a point from P2 (x,y)\n",
    "#returns an integer\n",
    "def length(x,y):\n",
    "    return(max(abs(np.subtract(x,y)))) \n",
    "\n",
    "#Point projected onto the diagonal\n",
    "#x: a point from Pn\n",
    "#returns point on the diagonal closest to x\n",
    "def diagonalProjection(x):\n",
    "    diag = (x[0] + x[1]) / 2\n",
    "    point = (diag,diag)\n",
    "    return point\n",
    "\n",
    "#Length of projection onto the diagonal\n",
    "#x: a point from Pn\n",
    "#returns an int\n",
    "def projectionLength(x):\n",
    "    pt = diagonalProjection(x)\n",
    "    dist = length(x,pt)\n",
    "    return dist\n",
    "\n",
    "#Nearest neighbor pairing inside loop\n",
    "#k: an ordered pair from P1\n",
    "#y: all points in P2\n",
    "#returns distance from point to neighbor and coordinates of neighbor\n",
    "def nearestNeighbor(k,y):\n",
    "    dist = float('inf')\n",
    "    pt = [-1,-1]\n",
    "    #check point in P1 against every point in P2\n",
    "    for i in y:\n",
    "        leng = length(k,i)\n",
    "        #find shortest distance between the points, store distance and neighbor\n",
    "        if leng < dist:\n",
    "            pt = i\n",
    "            dist = leng\n",
    "    leng = projectionLength(k)\n",
    "    #compare shortest length found above to distance from point to diagonal, \n",
    "    #keep shorter distance\n",
    "    if leng < dist:\n",
    "        pt = diagonalProjection(k)\n",
    "        dist = leng\n",
    "    return (dist,pt)\n",
    "\n",
    "#finds the shortest matching between points in two PD\n",
    "#x: data points in P1\n",
    "#y: data points in P2\n",
    "#returns the shortest match between P1 and P2 (distance, origin, neighbor)\n",
    "def bestMatch(x,y):\n",
    "    dist = float('inf')\n",
    "    neighbor = [-1,-1]\n",
    "    origin = [-1,-1]\n",
    "    #check point in P1 against every point in P2\n",
    "    for i in x:\n",
    "        (leng,pt) = nearestNeighbor(i,y)  #find nearest neighbor\n",
    "        #find shortest distance between points, store distance, origin, and neighbor\n",
    "        if leng < dist:            \n",
    "            dist = leng\n",
    "            neighbor = pt\n",
    "            origin = i\n",
    "        return(dist,origin,neighbor)\n",
    "\n",
    "#finds the total distance between two PD; one way\n",
    "#x: data points in P1\n",
    "#y: data points in P2\n",
    "#returns the list of matchings between PD\n",
    "#   and distance from first PD to second\n",
    "def distanceBetweenMatchings1(x,y):\n",
    "    listAnswer = []\n",
    "    totalList = []\n",
    "    while bool(x):\n",
    "        answer = bestMatch(x,y)     #find closest pair between P1 and P2\n",
    "        x.remove(answer[1])         #remove matched point from P1\n",
    "        if answer[2] in y: y.remove(answer[2])         #remove matched point from P2\n",
    "        listAnswer.append(answer)   #add point to final list\n",
    "        totalList.append(answer[0]) #add distance to distance list\n",
    "    average = 0\n",
    "    if len(totalList)!=0: average = sum(totalList)/len(totalList)          #add distance list together\n",
    "    return listAnswer, average\n",
    "\n",
    "#finds the total distance between two PD; both ways\n",
    "#x: data points in P1\n",
    "#y: data points in P2\n",
    "#returns the total distance between PD\n",
    "def distanceBetweenMatchings(x,y):\n",
    "    d1 = distanceBetweenMatchings1(x,y)\n",
    "    d2 = distanceBetweenMatchings1(y,x)\n",
    "    dist = (d1[1] + d2[1])/ 2              #average of (P1,P2) and (P2,P1)\n",
    "    return dist\n",
    "\n",
    "#small scale test\n",
    "x = (1,2)\n",
    "y = (3,7.4)\n",
    "\n",
    "length(x,y)\n",
    "\n",
    "print(diagonalProjection(x))\n",
    "\n",
    "print(projectionLength(x))\n",
    "\n",
    "k = (5,4)\n",
    "y = [(1,2),(5,6)]\n",
    "\n",
    "nearestNeighbor(k,y)\n",
    "\n",
    "x = [(1,2),(3,4)]\n",
    "y = [(1,2.1),(7,8)]\n",
    "\n",
    "#bestMatch(x,y)\n",
    "\n",
    "distanceBetweenMatchings1(x,y)\n",
    "\n",
    "print(distanceBetweenMatchings(x,y))"
   ]
