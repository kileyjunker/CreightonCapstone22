    "#create test data\n",
    "n=100\n",
    "data = np.random.random((n,2))\n",
    "    \n",
    "#function that runs PH, (visualizes PH), \n",
    "#and finds distance between two data sets\n",
    "#n: number of points in data set 1\n",
    "#m: number of points in data set 2\n",
    "#returns distance between data sets\n",
    "def testFunction(data,data1):\n",
    "    #run persistent homology on data\n",
    "    diagram = ripser(data)['dgms']\n",
    "    diagram1 = ripser(data1)['dgms']\n",
    "    \n",
    "    #visualize persistence diagrams\n",
    "#    plot_diagrams(diagram, show=True)\n",
    "#    plot_diagrams(diagram1, show=True)\n",
    "    \n",
    "    #extract H1 class (eliminating H0 elements)\n",
    "    dPD = diagram[1].tolist()\n",
    "    d1PD = diagram1[1].tolist()\n",
    "    \n",
    "    #calculate total distance between two persistence diagrams\n",
    "    return distanceBetweenMatchings(dPD,d1PD)\n",
    "\n",
    "#adds the same random integer 1-10 to every value in data\n",
    "#returns: translated data\n",
    "def translate(data):\n",
    "    dist = random.randrange(1,10)\n",
    "    rows = len(data)\n",
    "    cols = len(data[0])\n",
    "    size = rows*cols\n",
    "    mat = np.array([dist]*size).reshape(rows,cols)\n",
    "    data2 = data + mat\n",
    "    return data2\n",
    "\n",
    "#adds a different random of fixed magnitude and range to every value in data\n",
    "#low: the minimum of the range\n",
    "#high: the maximum of the range\n",
    "#mag: magnitude of preturbation\n",
    "#returns: perturbed data\n",
    "def perturbRanFixMag(data, low, high, mag):\n",
    "    rows = len(data)\n",
    "    cols = len(data[0])\n",
    "    mat = np.zeros((rows,cols))\n",
    "    for i in range(len(data)):\n",
    "        for j in range(len(data[0])):\n",
    "            dist = mag*random.randrange(low, high)\n",
    "            mat[i,j] = data[i,j] + dist\n",
    "    return mat\n",
    "\n",
    "#multiplies every value in data by the scale\n",
    "#returns: scaled data\n",
    "def scale(data, amt):\n",
    "    rows = len(data)\n",
    "    cols = len(data[0])\n",
    "    mat = np.zeros((rows,cols))\n",
    "    mat = amt*data\n",
    "    return mat\n",
    "\n",
    "#summarizes a list of numbers\n",
    "#returns: min, max, avg of aList\n",
    "def summarizeList(aList):\n",
    "#    print(aList)\n",
    "    print(\"min = \", min(aList))\n",
    "    print(\"max = \", max(aList))\n",
    "    print(\"avg = \", sum(aList)/len(aList))\n",
    "#    return min(aList), max(aList), sum(aList)/len(aList)"
   ]
